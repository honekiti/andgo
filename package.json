{
  "name": "hongo-v3",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --lan",
    "start:ios": "expo start --go --ios",
    "start:android": "expo start --go --android",
    "dev": "run-s clean:watchman start",
    "dev:ios": "run-s clean:watchman start:ios",
    "dev:android": "run-s clean:watchman start:android",
    "build:dev:all": "eas build --profile development --platform all",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:preview:all": "eas build --profile preview --platform all",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:preview:android": "eas build --profile preview --platform android --auto-submit",
    "build:prod:all": "eas build --profile production --platform all",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "install:dev:simulator:ios": "eas build:run -p ios",
    "install:dev:simulator:android": "eas build:run -p android",
    "test": "LOG_LEVEL=fatal vitest run",
    "test:watch": "LOG_LEVEL=fatal vitest watch",
    "test:coverage": "LOG_LEVEL=fatal vitest run --coverage",
    "clean:watchman": "./clean-watchman.sh"
  },
  "dependencies": {

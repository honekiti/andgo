{
  "name": "hongo-v3",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client --lan",
    "start:ios": "expo start --dev-client --ios",
    "start:android": "expo start --dev-client --android",
    "dev": "run-s clean:watchman start",
    "dev:ios": "run-s clean:watchman start:ios",
    "dev:android": "run-s clean:watchman start:android",
    "build:dev:all": "eas build --profile development --platform all",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:prod:all": "eas build --profile production --platform all --auto-submit",
    "build:prod:ios": "eas build --profile production --platform ios --auto-submit",
    "build:prod:android": "eas build --profile production --platform android --auto-submit",
    "install:dev:simulator:ios": "eas build:run -p ios",
    "install:dev:simulator:android": "eas build:run -p android",
    "test": "LOG_LEVEL=fatal vitest run",
    "test:watch": "LOG_LEVEL=fatal vitest watch",
    "test:coverage": "LOG_LEVEL=fatal vitest run --coverage",
    "clean:watchman": "./clean-watchman.sh"
  },
